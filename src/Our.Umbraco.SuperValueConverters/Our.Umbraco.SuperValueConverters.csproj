<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version Condition="'$(BUILD_BUILDNUMBER)' == ''">1.0.0.0</Version>
    <Version Condition="'$(BUILD_BUILDNUMBER)' != ''">$(BUILD_BUILDNUMBER)</Version>
    <Description>A collection of powerful property value converters for cleaner code in Umbraco</Description>
    <Authors>Callum Whyte</Authors>
    <Copyright>Copyright Â© 2022 Callum Whyte, and other contributors</Copyright>
    <PackageLicenseUrl>https://github.com/callumbwhyte/super-value-converters/blob/main/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/callumbwhyte/super-value-converters</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/callumbwhyte/super-value-converters/main/docs/img/logo.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/callumbwhyte/super-value-converters</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>umbraco converters models-builder</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Umbraco.Cms.Infrastructure" Version="[9.0.0,10.0.0)" />
  </ItemGroup>
</Project>